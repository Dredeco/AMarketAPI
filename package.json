{
   "devDependencies": {
      "@types/express": "^4.17.18",
      "@types/jest": "^29.5.5",
      "jest": "^29.7.0",
      "ts-jest": "^29.1.1",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.5.2",
      "ts-node": "10.7.0",
      "@types/node": "^16.11.10"
   },
   "dependencies": {
      "express": "^4.18.2",
      "reflect-metadata": "^0.1.13",
      "sqlite3": "^5.1.6",
      "typeorm": "0.3.17",
      "pg": "^8.4.0"
   },
   "scripts": {
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "dev": "ts-node-dev src/index.ts",
      "test": "jest",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:create": "npm run typeorm migration:create src/database/migrations/Product",
      "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/database/index.ts",
      "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/database/index.ts"
   }
